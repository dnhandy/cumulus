<p id="notice"><%= notice %></p>

<h1>Job Files</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Contents</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @job_files.each do |job_file| %>
      <tr>
        <td><%= job_file.name %></td>
        <td><%= job_file.contents %></td>
        <td><%= link_to 'Show', job_file %></td>
        <td><%= link_to 'Edit', edit_job_file_path(job_file) %></td>
        <td><%= link_to 'Destroy', job_file, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Job File', new_job_file_path %>
